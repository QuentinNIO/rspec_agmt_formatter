<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Augment-<%= repo %> Test Report</title>
  <link rel="icon" type="image/png" href="resources/AugmentJenkins.png" title="AugmentJenkins">

  <!-- Bootstrap -->
  <link href="resources/bootstrap-3.2.0-dist/css/bootstrap.min.css" rel="stylesheet">
  <link href="resources/bootstrap-3.2.0-dist/css/bootstrap-theme-yeti.min.css" rel="stylesheet">
  <link href="resources/styles.css" rel="stylesheet">

  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

  <script src="resources/jquery-1.11.1.min.js"></script>
  <script src="resources/bootstrap-3.2.0-dist/js/bootstrap.min.js"></script>
  <script src="resources/jscharts.js"></script>
</head>

<body>
<div class="container">
  <div class="bs-docs-section clearfix">
    <div class="row">
      <div class="col-lg-12">

        <div class="bs-component">
          <div class="navbar navbar-default">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <IMG STYLE="border: none;" SRC="resources/Header-<%= repo %>.png">
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="row">

    <div class="col-lg-12">

      <table class="table table-striped table-hover ">
        <thead>
        <tr>
          <th>Status</th>
          <th>Test</th>
          <th>Duration</th>
        </tr>
        </thead>
        <tbody>
        <% @examples.each do |example| %>
            <tr <%= "class='collapsable' tabindex='1'"%> >
              <td><span class="label <%= example.klass %>"><%= example.status %></span></td>
              <td>
                <div>
                  <% if example.has_spec? %>
                      <div class="panel <%= example.klass('panel-') %>">
                        <div class="panel-heading">
                          <h3 class="panel-title"> <%= example.full_description %></h3>
                        </div>
                        <div class="panel-body">
                          <%= example.spec %>
                        </div>
                      </div>
                  <% else %>
                      <%= example.full_description %>
                  <% end %>
                </div>      
                <% if example.has_exception? %>
                    <div class="panel panel-danger">
                        <div class="panel-heading">
                          <h3 class="panel-title"><%= example.exception.klass %></h3>
                        </div>
                        <div class="panel-body">
                          <%= example.exception.explanation %>
                          <h5><%= example.exception.backtrace_message %></h5>
                          <%= example.exception.highlighted_source %>
                        </div>
                      <p align="center">
                        <% if example.screenshot %>
                          <img style="max-height:500px;max-width:500px" src="<%= example.screenshot %>">
                        <% end %>
                        <% if example.screenshot2 %>
                            <img style="max-height:500px;max-width:500px" src="<%= example.screenshot2 %>">
                        <% end %>
                        <% if example.screenshot3 %>
                            <img style="max-height:500px;max-width:500px" src="<%= example.screenshot3 %>">
                        <% end %>
                        <% if example.screenshot4 %>
                            <img style="max-height:500px;max-width:500px" src="<%= example.screenshot4 %>">
                        <% end %>
                        <% if example.screenshot5 %>
                            <img style="max-height:500px;max-width:500px" src="<%= example.screenshot5 %>">
                        <% end %>
                      </p>
                    </div>
                <% else %>
                  <div class="panel panel-danger">
                    <div class="panel-heading">
                      <h3 class="panel-title">Screenshots</h3>
                    </div>
                    <p align="center">
                      <% if example.screenshot %>
                          <img style="max-height:500px;max-width:500px" src="<%= example.screenshot %>">
                      <% end %>
                      <% if example.screenshot2 %>
                          <img style="max-height:500px;max-width:500px" src="<%= example.screenshot2 %>">
                      <% end %>
                      <% if example.screenshot3 %>
                          <img style="max-height:500px;max-width:500px" src="<%= example.screenshot3 %>">
                      <% end %>
                      <% if example.screenshot4 %>
                          <img style="max-height:500px;max-width:500px" src="<%= example.screenshot4 %>">
                      <% end %>
                      <% if example.screenshot5 %>
                          <img style="max-height:500px;max-width:500px" src="<%= example.screenshot5 %>">
                      <% end %>
                    </p>
                  </div>
                <% end %>
              </td>
              <td><%= example.duration %></td>
            </tr>
        <% end %>
        </tbody>
      </table>

    </div>
  </div>

</div>
</body>
</html>